{
  "name": "cpu-sim",
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/A-C-Rosenfeldt/CPU-simulator.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.3",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "env": "0.0.2",
    "mocha": "^8.4.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
  },
  "mochaExplorer.files": "test/**/*.ts",
  "mochaExplorer.require": "ts-node/register",
  "mocha": {
    "mochaExplorer.logpanel:": true,
    "mochaExplorer.files": "test/**/*.ts",
    "mochaExplorer.require": "ts-node/register",
    "mochaExplorer": {
      "files": "test/**/*.ts",
      "require": "ts-node/register",
      "env": {
        "TS_NODE_PROJECT": "tsconfig.test.json"
      }
    },
    "require": "ts-node/register",
    "extensions": [
      "ts",
      "tsx"
    ],
    "spec": [
      "test/**/*.*"
    ],
    "watch-files": [
      "source"
    ]
  },
  "mochaExplorer": {
    "files": "test/**/*.ts",
    "require": "ts-node/register",
    "env": {
      "TS_NODE_PROJECT": "tsconfig.test.json",
      "TS_NODE_PREFER_TS_EXTS": true
    }
  },
  "scripts": {
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" TS_NODE_PREFER_TS_EXTS=\"true\" npm run childTest",
    "childTest": "cross-env-shell \"echo Hi $TS_NODE_PROJECT\" && mocha --require ts-node/register test/*.ts",
    "childTest_at": "cross-env-shell \"echo Hi $TS_NODE_PROJECT\" && ts-mocha test/*.ts",
    "testW": "node node_modules/mocha/bin/mocha",
    "childR": "mocha --require ts-node/register test/*.ts"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
